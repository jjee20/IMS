<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <ForceDesignerDPIUnaware>true</ForceDesignerDPIUnaware>
    <ApplicationHighDpiMode>SystemAware</ApplicationHighDpiMode>
    <StartupObject>PresentationLayer.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Json\barangay.json" />
    <None Remove="Json\city.json" />
    <None Remove="Json\philippine_provinces_cities_municipalities_and_barangays_2019v2.json" />
    <None Remove="Json\province.json" />
    <None Remove="Json\region.json" />
    <None Remove="Reports\BillTypeListReport.rdlc" />
    <None Remove="Reports\CustomerDetailReport.rdlc" />
    <None Remove="Reports\CustomerListReport.rdlc" />
    <None Remove="Reports\CustomerTypeListReport.rdlc" />
    <None Remove="Reports\InvoiceTypeListReport.rdlc" />
    <None Remove="Reports\PaymentTypeListReport.rdlc" />
    <None Remove="Reports\ProductTypeListReport.rdlc" />
    <None Remove="Reports\PurchaseTypeListReport.rdlc" />
    <None Remove="Reports\SalesTypeListReport.rdlc" />
    <None Remove="Reports\ShipmentTypeListReport.rdlc" />
    <None Remove="Reports\VendorTypeListReport.rdlc" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Json\barangay.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Json\city.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Json\province.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Json\region.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Reports\CustomerDetailReport.rdlc">
